{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.recipe.com/recipeNew.schema.json",
    "type": "object",
    "properties": {
      "recipeName": {
        "type": "string"
      },
      "prepTime": {
        "type": "number"
      },
      "cookingTime": {
        "type": "number"
      },
      "instructions": {
        "type": "string"
      },
      "mealType": {
        "type": "string"
      },
      "ingredientList": {
          "type": "array"
      }
    },
    "additionalProperties": false,
    "required": [
      "recipeName"
    ]
  }
  